{
  "name": "@semantic-release/npm",
  "description": "semantic-release plugin to publish a npm package",
  "version": "0.0.0-development",
  "author": "Pierre Vanduynslager (https://twitter.com/@pvdlg_)",
  "license": "MIT",
  "homepage": "https://github.com/semantic-release/npm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-release/npm.git"
  },
  "bugs": {
    "url": "https://github.com/semantic-release/npm/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.4"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "eslint",
    "pretest": "yarn run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "dependencies": {
    "@semantic-release/error": "2.2.0",
    "aggregate-error": "3.0.0",
    "execa": "1.0.0",
    "lodash": "4.17.11",
    "normalize-url": "4.0.0",
    "rc": "1.2.8",
    "read-pkg": "5.0.0",
    "registry-auth-token": "4.0.0"
  },
  "devDependencies": {
    "clear-module": "3.0.0",
    "codecov": "3.0.0",
    "commitizen": "3.0.0",
    "cz-conventional-changelog": "2.0.0",
    "delay": "4.0.0",
    "dockerode": "2.5.3",
    "file-url": "3.0.0",
    "get-stream": "5.0.0",
    "got": "9.0.0",
    "nock": "10.0.0",
    "nyc": "14.0.0",
    "p-retry": "4.0.0",
    "semantic-release": "15.0.0",
    "stream-buffers": "3.0.2",
    "tempy": "0.3.0"
  },
  "peerDependencies": {
    "semantic-release": ">=15.9.0 <16.0.0"
  },
  "keywords": [
    "npm",
    "publish",
    "registry",
    "semantic-release",
    "version"
  ],
  "contributors": [
    "Stephan BÃ¶nnemann <stephan@boennemann.me> (http://boennemann.me)",
    "Gregor Martynus (https://twitter.com/gr2m)"
  ],
  "// tooling configuration": "",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  }
}
